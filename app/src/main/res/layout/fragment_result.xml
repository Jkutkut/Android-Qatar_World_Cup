    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/txtvPhase"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:text="phase"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtvDateTime"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:text="22/22/2222 22:22"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtvTeam1"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:text="España"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/txtvPhase" />

        <TextView
            android:id="@+id/txtvScore1"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:layout_marginEnd="@dimen/normal_margin"
            android:text="10"
            app:layout_constraintEnd_toStartOf="@+id/separator"
            app:layout_constraintTop_toBottomOf="@id/txtvPhase" />

        <TextView
            android:id="@+id/separator"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:text="-"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/txtvPhase" />

        <TextView
            android:id="@+id/txtvScore2"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:layout_marginStart="@dimen/normal_margin"
            android:text="10"
            app:layout_constraintStart_toEndOf="@+id/separator"
            app:layout_constraintTop_toBottomOf="@id/txtvPhase" />

        <TextView
            android:id="@+id/txtvTeam2"
            style="@style/txtv_style"
            android:layout_width="wrap_content"
            android:text="España"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/txtvPhase" />
    </androidx.constraintlayout.widget.ConstraintLayout>

